+bfo:BFO_0000015 rdf:type owl:Class ;
+                obo:BFO_0000179 "process" ;
+                obo:BFO_0000180 "Process" ;
+                obo:IAO_0000112 "a process of cell-division, \\ a beating of the heart"@en ,
+                                "a process of meiosis"@en ,
+                                "a process of sleeping"@en ,
