<?xml version="1.0"?>
<rdf:RDF xmlns="http://purl.org/roh/mirror/bibo#"
     xml:base="http://purl.org/roh/mirror/bibo#"
     xmlns:ns="http://www.w3.org/2003/06/sw-vocab-status/ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:bibo="http://purl.org/roh/mirror/bibo#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:event="http://purl.org/NET/c4dm/event.owl#"
     xmlns:prism="http://prismstandard.org/namespaces/1.2/basic/"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:schema="http://schemas.talis.com/2005/address/schema#"
     xmlns:status="http://purl.org/roh/mirror/bibo#status/"
     xmlns:degrees="http://purl.org/roh/mirror/bibo#degrees/">
    <owl:Ontology rdf:about="http://purl.org/roh/mirror/bibo">
        <owl:versionIRI rdf:resource="http://purl.org/roh/mirror/bibo#1.3"/>
        <terms:creator rdf:resource="http://purl.org/roh/mirror/bibo#bdarcus"/>
        <terms:creator rdf:resource="http://purl.org/roh/mirror/bibo#fgiasson"/>
        <terms:description xml:lang="en">The Bibliographic Ontology describes
bibliographic things on the semantic Web in RDF.  This ontology can be
used as a citation ontology, as a document classification ontology, or
simply as a way to describe any kind of document in RDF. It has been
inspired by many existing document description metadata formats, and
can be used as a common ground for converting other bibliographic data
sources.</terms:description>
        <terms:title>The Bibliographic Ontology</terms:title>
        <owl:versionInfo>http://purl.org/roh/mirror/bibo#1.3/</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/terms/creator -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/creator"/>
    


    <!-- http://purl.org/dc/terms/description -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/description"/>
    


    <!-- http://purl.org/dc/terms/identifier -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/identifier"/>
    


    <!-- http://purl.org/dc/terms/title -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/title"/>
    


    <!-- http://www.w3.org/2002/07/owl#deprecated -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2002/07/owl#deprecated"/>
    


    <!-- http://www.w3.org/2003/06/sw-vocab-status/ns#term_status -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2003/06/sw-vocab-status/ns#term_status"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#changeNote -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#changeNote"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#editorialNote -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#editorialNote"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#example -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#example"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#historyNote -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#historyNote"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#note -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#note"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#scopeNote -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#scopeNote"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/NET/c4dm/event.owl#agent -->

    <owl:ObjectProperty rdf:about="http://purl.org/NET/c4dm/event.owl#agent">
        <skos:changeNote xml:lang="en">Used to link an agent (a person) to an event (a conference, an hearing, etc.)</skos:changeNote>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/c4dm/event.owl#place -->

    <owl:ObjectProperty rdf:about="http://purl.org/NET/c4dm/event.owl#place">
        <skos:scopeNote xml:lang="en">Used to relate an event such as a conference to the geographical place where it happens, for example Paris.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/c4dm/event.owl#produced_in -->

    <owl:ObjectProperty rdf:about="http://purl.org/NET/c4dm/event.owl#produced_in"/>
    


    <!-- http://purl.org/NET/c4dm/event.owl#product -->

    <owl:ObjectProperty rdf:about="http://purl.org/NET/c4dm/event.owl#product">
        <skos:scopeNote xml:lang="en">Used to link an event such as a conference to an outcome (a product) of that event, for example, an article, a proceeding, etc.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/c4dm/event.owl#sub_event -->

    <owl:ObjectProperty rdf:about="http://purl.org/NET/c4dm/event.owl#sub_event">
        <skos:scopeNote xml:lang="en">Used to link big events with smaller events such as workshops that happen in the context of a conference.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/c4dm/event.owl#time -->

    <owl:ObjectProperty rdf:about="http://purl.org/NET/c4dm/event.owl#time">
        <skos:scopeNote xml:lang="en">Used to describe the timing of an event. For example, when a conference starts and stops.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/terms/contributor -->

    <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/contributor">
        <skos:scopeNote xml:lang="en">Used to link a bibliographic item to one of its contributor: can be an author, an editor, a publisher, etc.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/terms/format -->

    <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/format">
        <skos:example xml:lang="en">&lt;dcterms:format&gt;
   &lt;dcterms:MediaTypeOrExtent&gt;
     &lt;rdf:value&gt;text/html&lt;/rdf:value&gt;
     &lt;rdfs:label&gt;HTML&lt;/rdfs:label&gt;
   &lt;/dcterms:MediaTypeOrExtent&gt;
 &lt;/dcterms:format&gt;</skos:example>
        <skos:scopeNote xml:lang="en">Used to describe the format of a bibliographic resource.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/terms/hasPart -->

    <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/hasPart"/>
    


    <!-- http://purl.org/dc/terms/isPartOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/isPartOf"/>
    


    <!-- http://purl.org/dc/terms/isReferencedBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/isReferencedBy">
        <skos:scopeNote xml:lang="en">Used to relate a reference citation to a bibliographic resource.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/terms/isVersionOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/isVersionOf"/>
    


    <!-- http://purl.org/dc/terms/language -->

    <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/language">
        <skos:scopeNote xml:lang="en">Used to link a bibliographic resource to the language used to express it.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/terms/publisher -->

    <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/publisher">
        <skos:scopeNote xml:lang="en">Used to link a bibliographic item to its publisher.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/terms/references -->

    <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/references"/>
    


    <!-- http://purl.org/dc/terms/relation -->

    <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/relation"/>
    


    <!-- http://purl.org/dc/terms/rights -->

    <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/rights">
        <skos:scopeNote xml:lang="en">Used to describe rights related to a bibliographic resource.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/terms/subject -->

    <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/subject">
        <skos:scopeNote xml:lang="en">Used to describe the subject of a bibliographic resource.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/terms/title -->

    <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/title"/>
    


    <!-- http://purl.org/roh/mirror/bibo#affirmedBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/roh/mirror/bibo#affirmedBy">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/roh/mirror/bibo#subsequentLegalDecision"/>
        <rdfs:domain rdf:resource="http://purl.org/roh/mirror/bibo#LegalDecision"/>
        <rdfs:range rdf:resource="http://purl.org/roh/mirror/bibo#LegalDecision"/>
        <rdfs:comment xml:lang="en">A legal decision that affirms a ruling.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://purl.org/roh/mirror/bibo#</rdfs:isDefinedBy>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/roh/mirror/bibo#annotates -->

    <owl:ObjectProperty rdf:about="http://purl.org/roh/mirror/bibo#annotates">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/relation"/>
        <rdfs:domain rdf:resource="http://purl.org/roh/mirror/bibo#Note"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
        <rdfs:comment xml:lang="en">Critical or explanatory note for a Document.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://purl.org/roh/mirror/bibo#</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">annotates</rdfs:label>
        <ns:term_status>stable</ns:term_status>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/roh/mirror/bibo#authorList -->

    <owl:ObjectProperty rdf:about="http://purl.org/roh/mirror/bibo#authorList">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/roh/mirror/bibo#contributorList"/>
        <rdfs:domain rdf:resource="http://purl.org/roh/mirror/bibo#Document"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                    <rdf:Description rdf:about="http://www.w3.org/1999/02/22-rdf-syntax-ns#Seq"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">An ordered list of authors. Normally, this list is seen as a priority list that order authors by importance.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://purl.org/roh/mirror/bibo#</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">list of authors</rdfs:label>
        <ns:term_status>stable</ns:term_status>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/roh/mirror/bibo#citedBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/roh/mirror/bibo#citedBy">
        <owl:inverseOf rdf:resource="http://purl.org/roh/mirror/bibo#cites"/>
        <rdfs:domain rdf:resource="http://purl.org/roh/mirror/bibo#Document"/>
        <rdfs:range rdf:resource="http://purl.org/roh/mirror/bibo#Document"/>
        <rdfs:comment xml:lang="en">Relates a document to another document that cites the
first document.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://purl.org/roh/mirror/bibo#</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">cited by</rdfs:label>
        <ns:term_status>unstable</ns:term_status>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/roh/mirror/bibo#cites -->

    <owl:ObjectProperty rdf:about="http://purl.org/roh/mirror/bibo#cites">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/references"/>
        <rdfs:domain rdf:resource="http://purl.org/roh/mirror/bibo#Document"/>
        <rdfs:range rdf:resource="http://purl.org/roh/mirror/bibo#Document"/>
        <rdfs:comment xml:lang="en">Relates a document to another document that is cited
by the first document as reference, comment, review, quotation or for
another purpose.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://purl.org/roh/mirror/bibo#</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">cites</rdfs:label>
        <ns:term_status>unstable</ns:term_status>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/roh/mirror/bibo#contributorList -->

    <owl:ObjectProperty rdf:about="http://purl.org/roh/mirror/bibo#contributorList">
        <rdfs:domain rdf:resource="http://purl.org/roh/mirror/bibo#Document"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                    <rdf:Description rdf:about="http://www.w3.org/1999/02/22-rdf-syntax-ns#Seq"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">An ordered list of contributors. Normally, this list is seen as a priority list that order contributors by importance.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://purl.org/roh/mirror/bibo#</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">list of contributors</rdfs:label>
        <ns:term_status>stable</ns:term_status>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/roh/mirror/bibo#court -->

    <owl:ObjectProperty rdf:about="http://purl.org/roh/mirror/bibo#court">
        <rdfs:domain rdf:resource="http://purl.org/roh/mirror/bibo#LegalDocument"/>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
        <rdfs:comment xml:lang="en">A court associated with a legal document; for example, that which issues a decision.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://purl.org/roh/mirror/bibo#</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">court</rdfs:label>
        <ns:term_status>unstable</ns:term_status>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/roh/mirror/bibo#degree -->

    <owl:ObjectProperty rdf:about="http://purl.org/roh/mirror/bibo#degree">
        <rdfs:domain rdf:resource="http://purl.org/roh/mirror/bibo#Thesis"/>
        <rdfs:range rdf:resource="http://purl.org/roh/mirror/bibo#ThesisDegree"/>
        <rdfs:comment xml:lang="en">The thesis degree.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://purl.org/roh/mirror/bibo#</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">degree</rdfs:label>
        <ns:term_status>unstable</ns:term_status>
        <skos:editorialNote xml:lang="en">We are not defining, using an enumeration, the range of the bibo:degree to the defined list of bibo:ThesisDegree. We won&apos;t do it because we want people to be able to define new degress if needed by some special usecases. Creating such an enumeration would restrict this to happen.</skos:editorialNote>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/roh/mirror/bibo#director -->

    <owl:ObjectProperty rdf:about="http://purl.org/roh/mirror/bibo#director">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/contributor"/>
        <rdfs:domain rdf:resource="http://purl.org/roh/mirror/bibo#AudioVisualDocument"/>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
        <rdfs:comment xml:lang="en">A Film director.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://purl.org/roh/mirror/bibo#</rdfs:isDefinedBy>
        <rdfs:label>director</rdfs:label>
        <ns:term_status>stable</ns:term_status>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/roh/mirror/bibo#distributor -->

    <owl:ObjectProperty rdf:about="http://purl.org/roh/mirror/bibo#distributor">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/roh/mirror/bibo#Collection"/>
                    <rdf:Description rdf:about="http://purl.org/roh/mirror/bibo#Document"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
        <rdfs:comment xml:lang="en">Distributor of a document or a collection of documents.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://purl.org/roh/mirror/bibo#</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">distributor</rdfs:label>
        <ns:term_status>stable</ns:term_status>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/roh/mirror/bibo#editor -->

    <owl:ObjectProperty rdf:about="http://purl.org/roh/mirror/bibo#editor">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/contributor"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/roh/mirror/bibo#Collection"/>
                    <rdf:Description rdf:about="http://purl.org/roh/mirror/bibo#Document"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
        <rdfs:comment xml:lang="en">A person having managerial and sometimes policy-making responsibility for the editorial part of a publishing firm or of a newspaper, magazine, or other publication.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://purl.org/roh/mirror/bibo#</rdfs:isDefinedBy>
        <rdfs:label>editor</rdfs:label>
        <ns:term_status>stable</ns:term_status>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/roh/mirror/bibo#editorList -->

    <owl:ObjectProperty rdf:about="http://purl.org/roh/mirror/bibo#editorList">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/roh/mirror/bibo#contributorList"/>
        <rdfs:domain rdf:resource="http://purl.org/roh/mirror/bibo#Document"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                    <rdf:Description rdf:about="http://www.w3.org/1999/02/22-rdf-syntax-ns#Seq"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">An ordered list of editors. Normally, this list is seen as a priority list that order editors by importance.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://purl.org/roh/mirror/bibo#</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">list of editors</rdfs:label>
        <ns:term_status>stable</ns:term_status>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/roh/mirror/bibo#interviewee -->

    <owl:ObjectProperty rdf:about="http://purl.org/roh/mirror/bibo#interviewee">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/contributor"/>
        <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
        <rdfs:comment xml:lang="en">An agent that is interviewed by another agent.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://purl.org/roh/mirror/bibo#</rdfs:isDefinedBy>
        <rdfs:label>interviewee</rdfs:label>
        <ns:term_status>stable</ns:term_status>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/roh/mirror/bibo#interviewer -->

    <owl:ObjectProperty rdf:about="http://purl.org/roh/mirror/bibo#interviewer">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/contributor"/>
        <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
        <rdfs:comment xml:lang="en">An agent that interview another agent.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://purl.org/roh/mirror/bibo#</rdfs:isDefinedBy>
        <rdfs:label>interviewer</rdfs:label>
        <ns:term_status>stable</ns:term_status>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/roh/mirror/bibo#issuer -->

    <owl:ObjectProperty rdf:about="http://purl.org/roh/mirror/bibo#issuer">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/publisher"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/roh/mirror/bibo#Collection"/>
                    <rdf:Description rdf:about="http://purl.org/roh/mirror/bibo#Document"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
        <rdfs:comment>An entity responsible for issuing often informally published documents such as press releases, reports, etc.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://purl.org/roh/mirror/bibo#</rdfs:isDefinedBy>
        <rdfs:label>issuer</rdfs:label>
        <ns:term_status>unstable</ns:term_status>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/roh/mirror/bibo#organizer -->

    <owl:ObjectProperty rdf:about="http://purl.org/roh/mirror/bibo#organizer">
        <rdfs:domain rdf:resource="http://purl.org/NET/c4dm/event.owl#Event"/>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
