###  http://purl.org/roh#country
:country rdf:type owl:ObjectProperty ;
         rdfs:range [ owl:intersectionOf ( skos:Concept
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty skos:inScheme ;
                                             owl:someValuesFrom :Country
                                           ]
                                         ) ;
                      rdf:type owl:Class
                    ] ;
         rdfs:comment "This property indicates the nationality of a resource. The domain is not set so unpredicted resources within the ontology could be attached to countries."@en ;
         rdfs:label "country"@en ,
                    "país"@es .


###  http://purl.org/roh#documentStatus
:documentStatus rdf:type owl:ObjectProperty ;
                rdfs:domain bibo:Document ;
                rdfs:range [ rdf:type owl:Class ;
                             owl:unionOf ( :Accepted
                                           :Rejected
                                         )
                           ] ;
                rdfs:comment "The status of a Document (Accepted or Rejected)."@en ;
                rdfs:label "estado"@es ,
                           "status"@en .


###  http://purl.org/roh#employs
:employs rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf obo:RO_0000057 ;
         rdfs:domain :ResearchContribution ;
         rdfs:range :ResearchMethod ;
         rdfs:label "emplea"@es ,
                    "employs"@en .


###  http://purl.org/roh#evaluationStatus
:evaluationStatus rdf:type owl:ObjectProperty ;
