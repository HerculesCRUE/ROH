###  http://purl.org/dc/terms/hasPart
terms:hasPart rdf:type owl:ObjectProperty ;
              rdfs:comment "A related resource that is included either physically or logically in the described resource."@en ;
              rdfs:label "está compuesto por"@es ,
                         "has part"@en .


###  http://purl.org/roh#CVOf
:CVOf rdf:type owl:ObjectProperty ;
      owl:inverseOf :hasCV ;
      rdfs:domain :CurriculumVitae ;
      rdfs:range <http://purl.org/roh/mirror/foaf#Person> ;
      rdfs:comment "An object property linking a CV to the person it belongs to."@en ;
      rdfs:label "CV de"@es ,
                 "CV of"@en .


###  http://purl.org/roh#accreditationOf
:accreditationOf rdf:type owl:ObjectProperty ;
                 owl:inverseOf :hasAccreditation ;
                 rdfs:domain :Accreditation ;
                 rdfs:range <http://purl.org/roh/mirror/foaf#Agent> ;
                 rdfs:comment "An object property linking an Accreditation to the Agent is associated with."@en ;
                 rdfs:label "accreditation of"@en ,
                            "acreditación de"@es .


###  http://purl.org/roh#addresses
:addresses rdf:type owl:ObjectProperty ;
           rdfs:domain :ResearchContribution ;
           rdfs:range [ owl:intersectionOf ( skos:Concept
                                             [ rdf:type owl:Restriction ;
                                               owl:onProperty skos:inScheme ;
                                               owl:someValuesFrom :ResearchProblem
                                             ]
                                           ) ;
                        rdf:type owl:Class
                      ] ;
           rdfs:label "addresses"@en .


###  http://purl.org/roh#advises
:advises rdf:type owl:ObjectProperty ;
         owl:inverseOf :isAdvisedBy ;
         rdfs:domain <http://purl.org/roh/mirror/foaf#Person> ;
         rdfs:range bibo:Document ;
         rdfs:label "advises"@en .


###  http://purl.org/roh#associatedWith
:associatedWith rdf:type owl:ObjectProperty ,
                         owl:SymmetricProperty ;
                rdfs:domain <http://purl.org/roh/mirror/foaf#Agent> ;
                rdfs:range <http://purl.org/roh/mirror/foaf#Agent> ;
                rdfs:comment "an organisation (i.e. university) is associated with another organsiation (i.e. research institute)."@en ;
                rdfs:label "associated with"@en .


###  http://purl.org/roh#attachment
:attachment rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:comment "An object property describing a not machine-readable document attached to an entity offering more detailed information."@en ;
            rdfs:label "adjunto"@es ,
                       "attachment"@en .


###  http://purl.org/roh#authors
:authors rdf:type owl:ObjectProperty ;
         owl:inverseOf :isAuthoredBy ;
         rdfs:domain <http://purl.org/roh/mirror/foaf#Agent> ;
         rdfs:range bibo:Document ;
         rdfs:label "authors"@en .


###  http://purl.org/roh#awardedBy
:awardedBy rdf:type owl:ObjectProperty ;
           owl:inverseOf :awards ;
