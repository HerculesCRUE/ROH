                            vivo:start [ a vivo:DateTimeValue ;
                                    vivo:dateTime "2015-01-01T00:00:00+00:00"^^xsd:dateTime ] ] ;
                    roh:monetaryAmount "24800.00" ] ;
            roh:fundedBy <http://data.example.org/fundingprogram/5> ],
        [ a roh:Funding ;
            ro:BFO_0000051 [ a roh:FundingAmount ;
                    vivo:dateTimeInterval [ a vivo:DateTimeInterval ;
                            vivo:end [ a vivo:DateTimeValue ;
