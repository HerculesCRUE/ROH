PREFIX vivo: <http://purl.org/roh_edma/mirror/vivo#>
PREFIX roh: <http://purl.org/roh_edma#>
prefix ro: <http://purl.org/roh_edma/mirror/obo/ro#> 
SELECT  distinct ?project ?centre 
WHERE {
?project a vivo:Project ;
		vivo:relates ?role ;
		vivo:relates ?centreRole .
?role ro:RO_0000052 <http://purl.org/roh_edma/data#company-one> .
?centreRole ro:RO_0000052 ?centre .
?centre a ?ClassCentre.
FILTER(?ClassCentre = roh:ResearchGroup || ?ClassCentre = roh:ResearchInstitute)
}
