PREFIX iao: <http://purl.org/roh_edma/mirror/obo/iao#>
PREFIX roh: <http://purl.org/roh_edma#>
PREFIX bibo:  <http://purl.org/roh_edma/mirror/bibo#>
PREFIX rdfs:  <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX vivo: <http://purl.org/roh_edma/mirror/vivo#>
select  ?journal (year(?date) as ?year) ?quartile ?impactFactor ?citationCount 
where{ 
    ?journal a iao:IAO_0000013 ;
         roh:hasMetric ?metric. 
    ?metric a roh:PublicationMetric;
             roh:impactFactor ?impactFactor ;
        	roh:citationCount ?citationCount;
         	roh:metricName "jcr";
        	roh:quartile ?quartile;
               vivo:dateIssued ?dateNode.      
  ?dateNode vivo:dateTime ?date.   

}group by ?journal ?date ?quartile ?impactFactor ?citationCount ?date 
order by ?journal