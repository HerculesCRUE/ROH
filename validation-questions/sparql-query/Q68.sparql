PREFIX roh: <http://w3id.org/roh#>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> 

select   ?researchResult ?resource 
where {
    ?researchResult a roh:ResearchResult;
                    roh:correspondingAuthor <http://w3id.org/roh/data#investigador-1> ;
                    roh:researchResultHasPart ?resource.
    
}
