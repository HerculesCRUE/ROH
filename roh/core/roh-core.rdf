    <!-- http://w3id.org/roh#EuropeanAgriculturalGuaranteeFund -->

    <owl:NamedIndividual rdf:about="http://w3id.org/roh#EuropeanAgriculturalGuaranteeFund">
        <rdf:type rdf:resource="http://w3id.org/roh#FundingSource"/>
        <rdfs:comment xml:lang="en">A fund allocated by the European Union. It finances direct payments to farmers under the Common Agricultural Policy (CAP) and measures to regulate agricultural markets such as intervention and export refunds.</rdfs:comment>
