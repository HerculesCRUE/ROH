    <!-- http://w3id.org/roh#EuropeanRegionalDevelopmentFund -->

    <owl:NamedIndividual rdf:about="http://w3id.org/roh#EuropeanRegionalDevelopmentFund">
        <rdf:type rdf:resource="http://w3id.org/roh#FundingSource"/>
        <rdfs:comment xml:lang="en">A fund allocated by the European Union. Its purpose is to transfer money from richer regions (not countries), and invest it in the infrastructure and services of underdeveloped regions. This will allow those regions to start attracting private sector investments, and create jobs on their own.</rdfs:comment>
