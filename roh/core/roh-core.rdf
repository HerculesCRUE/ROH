    <owl:ObjectProperty rdf:about="http://w3id.org/roh#addresses">
        <rdfs:domain rdf:resource="http://w3id.org/roh#ResearchContribution"/>
        <rdfs:range>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://w3id.org/roh/mirror/skos#Concept"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://w3id.org/roh/mirror/skos#inScheme"/>
                        <owl:someValuesFrom rdf:resource="http://w3id.org/roh#ResearchProblem"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:label xml:lang="en">addresses</rdfs:label>
    </owl:ObjectProperty>
    


