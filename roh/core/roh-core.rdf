    <!-- http://w3id.org/roh#associatedWith -->

    <owl:ObjectProperty rdf:about="http://w3id.org/roh#associatedWith">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="http://w3id.org/roh/mirror/foaf#Agent"/>
        <rdfs:range rdf:resource="http://w3id.org/roh/mirror/foaf#Agent"/>
