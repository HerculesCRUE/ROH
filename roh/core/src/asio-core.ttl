@prefix : <http://purl.org/hercules/asio/core#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix ro: <http://purl.org/wf4ever/ro#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix deo: <http://purl.org/spar/deo/> .
@prefix org: <http://www.w3.org/ns/org#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix pso: <http://purl.org/spar/pso/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix adms: <http://www.w3.org/ns/adms#> .
@prefix asio: <http://purl.org/hercules/asio/core#> .
@prefix bibo: <http://purl.org/ontology/bibo/> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix doco: <http://purl.org/spar/doco/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix vivo: <http://vivoweb.org/ontology/core#> .
@prefix cerif: <http://purl.org/cerif/> .
@prefix fabio: <http://purl.org/spar/fabio/> .
@prefix frapo: <http://purl.org/cerif/frapo#> .
@prefix modules: <http://purl.org/hercules/asio/modules#> .
@prefix semcerif: <http://eurocris.org/ontology/semcerif#> .
@prefix timezone: <http://www.w3.org/2006/timezone#> .
@prefix humantechreadiness: <http://sweetontology.net/humanTechReadiness#> .
@base <http://purl.org/hercules/asio/core#> .

<http://purl.org/hercules/asio/core#> rdf:type owl:Ontology ;
                                       owl:versionIRI <http://purl.org/hercules/asio/core/0.0.1> ;
                                       dc:contributor "Alejandro González Hevia"^^xsd:string ,
                                                      "José Emilio Labra Gayo"^^xsd:string ;
                                       dc:creator "Emilio Rubiera Azcona"^^xsd:string ;
                                       dc:description "An ontology to address the Research Management of the CRUE's Spanish University System (Sistema Universitario Español) by applying an encompassing model not only capable of addressing the universities of the CRUE but also more belonging to the European Union."^^xsd:string ;
                                       dc:title "The ASIO ontology"^^xsd:string ;
                                       dct:license <https://creativecommons.org/publicdomain/zero/1.0/> ;
                                       owl:versionInfo "0.0.1"^^xsd:string .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/contributor
dc:contributor rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
dc:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
dct:license rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#altLabel
skos:altLabel rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#prefLabel
skos:prefLabel rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://purl.org/hercules/asio/core#addresses
asio:addresses rdf:type owl:ObjectProperty ;
               rdfs:domain asio:ResearchContribution ;
               rdfs:range asio:ResearchProblem ;
               rdfs:label "addresses"@en .


###  http://purl.org/hercules/asio/core#advises
asio:advises rdf:type owl:ObjectProperty ;
             owl:inverseOf asio:isAdvisedBy ;
             rdfs:domain foaf:Person ;
             rdfs:range asio:AcademicPublication ;
             rdfs:label "advises"@en .


###  http://purl.org/hercules/asio/core#associatedWith
asio:associatedWith rdf:type owl:ObjectProperty ,
                             owl:SymmetricProperty ;
                    rdfs:domain foaf:Agent ;
                    rdfs:range foaf:Agent ;
                    rdfs:comment "an organisation (i.e. university) is associated with another organsiation (i.e. research institute)."@en ;
                    rdfs:label "associated with"@en .


###  http://purl.org/hercules/asio/core#atContext
asio:atContext rdf:type owl:ObjectProperty ;
               rdfs:domain asio:Profile ;
               rdfs:range asio:Context ;
               rdfs:comment "This property enables to relate a profile with a given context (geographical, temporal, spatial, social, etc.)."@en ;
               rdfs:label "at context"@en .


###  http://purl.org/hercules/asio/core#authorCitation
asio:authorCitation rdf:type owl:ObjectProperty ;
                    rdfs:domain foaf:Person ;
                    rdfs:range asio:QualifiedValue ;
                    rdfs:comment "A property to link the number of citations to a given author."@en ;
                    rdfs:label "author citations"@en .


###  http://purl.org/hercules/asio/core#authors
asio:authors rdf:type owl:ObjectProperty ;
             owl:inverseOf asio:isAuthoredBy ;
             rdfs:domain foaf:Agent ;
             rdfs:range asio:Publication ;
             rdfs:label "authors"@en .


###  http://purl.org/hercules/asio/core#awards
asio:awards rdf:type owl:ObjectProperty ;
            owl:inverseOf asio:isAwardedBy ;
            rdf:type owl:InverseFunctionalProperty ;
            rdfs:domain foaf:Agent ;
            rdfs:comment "An object property linking an agent to something the agent awards, for example linking a funding agency to a grant, a university to a degree, or an organization to a prize. [source: cerif/frapo]"@en ;
            rdfs:label "awards"@en .


###  http://purl.org/hercules/asio/core#capital
asio:capital rdf:type owl:ObjectProperty ;
             rdfs:domain skos:Concept ;
             rdfs:range skos:Concept ;
             rdfs:comment "A property to indicate that a place is the official seat of government in a political entity."@en ;
             rdfs:label "capital"@en .


###  http://purl.org/hercules/asio/core#cites
asio:cites rdf:type owl:ObjectProperty ;
           rdfs:domain asio:Publication ;
           rdfs:range asio:Publication ;
           rdfs:comment "a property to indicate that a document cites another one."@en ;
           rdfs:label "cites"@en .


###  http://purl.org/hercules/asio/core#coadvisedBy
asio:coadvisedBy rdf:type owl:ObjectProperty ;
                 rdfs:domain asio:AcademicPublication ;
                 rdfs:range foaf:Person ;
                 rdfs:comment "An academic publication (bachelor thesis, master thesis or doctoral thesis) is co-advised by an advisor."@en ;
                 rdfs:label "co-advised by"@en .


###  http://purl.org/hercules/asio/core#coadvises
asio:coadvises rdf:type owl:ObjectProperty ;
               rdfs:domain foaf:Person ;
               rdfs:range asio:AcademicPublication ;
               rdfs:comment "A person co-advises an academic publication (bachelor thesis, master thesis or doctoral thesis)."@en ;
               rdfs:label "co-advises"@en .


###  http://purl.org/hercules/asio/core#coauthors
asio:coauthors rdf:type owl:ObjectProperty ;
               rdfs:domain foaf:Agent ;
               rdfs:range asio:Publication ;
               rdfs:label "co-authors"@en .


###  http://purl.org/hercules/asio/core#codeOf
asio:codeOf rdf:type owl:ObjectProperty ;
            owl:inverseOf asio:hasCode ;
            rdfs:domain skos:Concept ;
            rdfs:range skos:Concept ;
            rdfs:comment "A code is assigned to a concept."@en ;
            rdfs:label "code of"@en .


###  http://purl.org/hercules/asio/core#companyFounder
asio:companyFounder rdf:type owl:ObjectProperty ;
                    rdfs:domain foaf:Person ;
                    rdfs:range asio:Company ;
                    rdfs:label "company founder"@en .


###  http://purl.org/hercules/asio/core#coordinatedBy
asio:coordinatedBy rdf:type owl:ObjectProperty ;
                   owl:inverseOf asio:projectCoordinator ;
                   rdfs:domain foaf:Project ;
                   rdfs:range foaf:Agent ;
                   rdfs:comment "inverse property of 'project coordinator'"@en ;
                   rdfs:label "coordinated by"@en .


###  http://purl.org/hercules/asio/core#country
asio:country rdf:type owl:ObjectProperty ;
             rdfs:range asio:Country ;
             rdfs:comment "This property indicates the nationality of a resource. The domain is not set so unpredicted resources within the ontology could be attached to countries."@en ;
             rdfs:label "country"@en ,
                        "país"@es .


###  http://purl.org/hercules/asio/core#employs
asio:employs rdf:type owl:ObjectProperty ;
             rdfs:domain asio:ResearchContribution ;
             rdfs:range asio:ResearchMethod ;
             rdfs:label "emplea"@es ,
                        "employs"@en .


###  http://purl.org/hercules/asio/core#funds
asio:funds rdf:type owl:ObjectProperty ;
           owl:inverseOf asio:isFundedBy ;
           rdfs:domain foaf:Agent ;
           rdfs:label "funds"@en .


###  http://purl.org/hercules/asio/core#geoDivision
asio:geoDivision rdf:type owl:ObjectProperty ;
                 rdfs:domain skos:Concept ;
                 rdfs:range modules:GeopoliticalEntities ;
                 rdfs:comment "A property to link a resource to a predefined geographical (or geopolitical) division."@en ;
                 rdfs:label "geodivision"@en .


###  http://purl.org/hercules/asio/core#h-index
asio:h-index rdf:type owl:ObjectProperty ;
             rdfs:domain foaf:Person ;
             rdfs:range asio:QualifiedValue ;
             rdfs:label "A property to express the h-index of an author."@en ,
                        "h-index"@en .


###  http://purl.org/hercules/asio/core#hasCV
asio:hasCV rdf:type owl:ObjectProperty ;
           rdfs:domain foaf:Person ;
           rdfs:range asio:CV ;
           rdfs:label "has CV"@en .


###  http://purl.org/hercules/asio/core#hasCode
asio:hasCode rdf:type owl:ObjectProperty ;
             rdfs:domain skos:Concept ;
             rdfs:range skos:Concept ;
             rdfs:comment "A concept can be asigned a code."@en ;
             rdfs:label "has Code"@en .


###  http://purl.org/hercules/asio/core#hasPatent
asio:hasPatent rdf:type owl:ObjectProperty ;
               rdfs:domain asio:Invention ;
               rdfs:range asio:Patent ;
               rdfs:label "has patent"@en .


###  http://purl.org/hercules/asio/core#hasResearchContribution
asio:hasResearchContribution rdf:type owl:ObjectProperty ;
                             rdfs:label "has research contribution"@en .


###  http://purl.org/hercules/asio/core#hasResearchField
asio:hasResearchField rdf:type owl:ObjectProperty ;
                      rdfs:label "has research field"@en .


###  http://purl.org/hercules/asio/core#hasResearchLine
asio:hasResearchLine rdf:type owl:ObjectProperty ;
                     rdfs:domain foaf:Agent ;
                     rdfs:range skos:Concept ;
                     rdfs:comment "a property linking organizations or groups with scientific domains"@en ;
                     rdfs:label "has research line"@en .


###  http://purl.org/hercules/asio/core#hasScientificDomain
asio:hasScientificDomain rdf:type owl:ObjectProperty ;
                         rdfs:range skos:Concept ;
                         rdfs:label "has scientific domain"@en .


###  http://purl.org/hercules/asio/core#hasSealOfQuality
asio:hasSealOfQuality rdf:type owl:ObjectProperty ;
                      rdfs:domain foaf:Agent ;
                      rdfs:range asio:ResearchInstitute ;
                      rdfs:label "has seal of quality"@en .


###  http://purl.org/hercules/asio/core#i10-index
asio:i10-index rdf:type owl:ObjectProperty ;
               rdfs:domain foaf:Person ;
               rdfs:range asio:QualifiedValue ;
               rdfs:comment "A property to express the i10-index of an author."@en ;
               rdfs:label "i10-index"@en .


###  http://purl.org/hercules/asio/core#invoiceOf
asio:invoiceOf rdf:type owl:ObjectProperty ;
               rdfs:domain asio:Invoice ;
               rdfs:range asio:Expenditure .


###  http://purl.org/hercules/asio/core#isAdvisedBy
asio:isAdvisedBy rdf:type owl:ObjectProperty ;
                 rdfs:label "is advised by"@en .


###  http://purl.org/hercules/asio/core#isAuthoredBy
asio:isAuthoredBy rdf:type owl:ObjectProperty ;
                  rdfs:label "is authored by"@en .


###  http://purl.org/hercules/asio/core#isAwardedBy
asio:isAwardedBy rdf:type owl:ObjectProperty ,
                          owl:FunctionalProperty ;
                 rdfs:range foaf:Agent ;
                 rdfs:label "is awarded by"@en .


###  http://purl.org/hercules/asio/core#isCoauthoredBy
asio:isCoauthoredBy rdf:type owl:ObjectProperty ;
                    rdfs:domain asio:Publication ;
                    rdfs:range foaf:Agent ;
                    rdfs:label "is co-authored by"@en .


###  http://purl.org/hercules/asio/core#isFundedBy
asio:isFundedBy rdf:type owl:ObjectProperty ;
                rdfs:label "is funded by"@en .


###  http://purl.org/hercules/asio/core#isGrantedWith
asio:isGrantedWith rdf:type owl:ObjectProperty ;
                   rdfs:domain foaf:Person ;
                   rdfs:range asio:Grant ;
                   rdfs:comment "A person is granted with a stipend for researching."@en ;
                   rdfs:label "is granted with"@en .


###  http://purl.org/hercules/asio/core#isMemberOf
asio:isMemberOf rdf:type owl:ObjectProperty ;
                owl:inverseOf foaf:member ;
                rdfs:label "is member of"@en .


###  http://purl.org/hercules/asio/core#mainResearcherParticipant
asio:mainResearcherParticipant rdf:type owl:ObjectProperty ;
                               rdfs:domain foaf:Person ;
                               rdfs:range foaf:Project ;
                               rdfs:comment "A person is the main researcher in a given project."@en ;
                               rdfs:label "main researcher participant"@en .


###  http://purl.org/hercules/asio/core#participant
asio:participant rdf:type owl:ObjectProperty ;
                 rdfs:domain foaf:Agent ;
                 rdfs:label "participant"@en .


###  http://purl.org/hercules/asio/core#participates
asio:participates rdf:type owl:ObjectProperty ;
                  rdfs:comment "This property encodes the participation relation between profiles and the communities they are member of."@en ;
                  rdfs:label "participates"@en .


###  http://purl.org/hercules/asio/core#patentAssignee
asio:patentAssignee rdf:type owl:ObjectProperty ;
                    rdfs:domain asio:Patent ;
                    rdfs:range foaf:Organization ;
                    rdfs:label "patent assignee"@en .


###  http://purl.org/hercules/asio/core#patentAssistantExaminer
asio:patentAssistantExaminer rdf:type owl:ObjectProperty ;
                             rdfs:domain foaf:Person ;
                             rdfs:range asio:Patent ;
                             rdfs:label "patent assistant examiner"@en .


###  http://purl.org/hercules/asio/core#patentCountry
asio:patentCountry rdf:type owl:ObjectProperty ;
                   rdfs:domain asio:Patent ;
                   rdfs:range asio:Country ;
                   rdfs:label "patent country"@en .


###  http://purl.org/hercules/asio/core#patentInventor
asio:patentInventor rdf:type owl:ObjectProperty ;
                    rdfs:domain asio:Patent ;
                    rdfs:range foaf:Person ;
                    rdfs:label "patent inventor"@en .


###  http://purl.org/hercules/asio/core#patentPrimaryExaminer
asio:patentPrimaryExaminer rdf:type owl:ObjectProperty ;
                           rdfs:domain foaf:Person ;
                           rdfs:range asio:Patent ;
                           rdfs:label "patent primary examiner"@en .


###  http://purl.org/hercules/asio/core#patentStatus
asio:patentStatus rdf:type owl:ObjectProperty ;
                  rdfs:domain asio:Patent ;
                  rdfs:range asio:Status ;
                  rdfs:label "patent status"@en .


###  http://purl.org/hercules/asio/core#personalizes
asio:personalizes rdf:type owl:ObjectProperty ;
                  rdfs:domain asio:Profile ;
                  rdfs:range asio:Role ;
                  rdfs:comment "A role is personalized by means of a profile, which wrap the role in the identity of the person behind the profile."@en ;
                  rdfs:label "personalizes"@en .


###  http://purl.org/hercules/asio/core#plays
asio:plays rdf:type owl:ObjectProperty ;
           rdfs:domain foaf:Agent ;
           rdfs:range asio:Role ;
           rdfs:comment "This property relates roles and their players, who can be any foaf:Agent (a person, a group, etc.)"@en ;
           rdfs:label "plays"@en .


###  http://purl.org/hercules/asio/core#profiles
asio:profiles rdf:type owl:ObjectProperty ;
              rdfs:domain foaf:Agent ;
              rdfs:range asio:Profile ;
              rdfs:comment "This property relates people and the featured profiles."@en ;
              rdfs:label "profiles"@en .


###  http://purl.org/hercules/asio/core#projectCoordinator
asio:projectCoordinator rdf:type owl:ObjectProperty ;
                        rdfs:domain foaf:Agent ;
                        rdfs:range foaf:Project ;
                        rdfs:comment "A property to link an agent (person or organisation) with a project in the role of coordinator."@en ;
                        rdfs:label "project coordinator"@en .


###  http://purl.org/hercules/asio/core#projectOutcome
asio:projectOutcome rdf:type owl:ObjectProperty ;
                    rdfs:domain foaf:Project ;
                    rdfs:comment "a property to link a project to any outcome obtained within its endeavour, including publications, patents, products, prototypes, etc. The domain of this property is obviously the class foaf:Project but the range is left open so to encompass unforeseen project outcomes."@en ;
                    rdfs:label "project outcome"@en .


###  http://purl.org/hercules/asio/core#projectStatus
asio:projectStatus rdf:type owl:ObjectProperty ;
                   rdfs:domain foaf:Project ;
                   rdfs:range asio:Status ;
                   rdfs:label "project status"@en .


###  http://purl.org/hercules/asio/core#publicationCitation
asio:publicationCitation rdf:type owl:ObjectProperty ;
                         rdfs:domain asio:Publication ;
                         rdfs:range asio:QualifiedValue ;
                         rdfs:comment "a property to link a given publication to the number of citations."@en ;
                         rdfs:label "publication citations"@en .


###  http://purl.org/hercules/asio/core#publishedBy
asio:publishedBy rdf:type owl:ObjectProperty ;
                 owl:inverseOf asio:publishes ;
                 rdfs:domain asio:Publication ;
                 rdfs:range foaf:Agent ;
                 rdfs:label "published by"@en .


###  http://purl.org/hercules/asio/core#publishes
asio:publishes rdf:type owl:ObjectProperty ;
               rdfs:domain foaf:Agent ;
               rdfs:range asio:Publication ;
               rdfs:label "publishes"@en .


###  http://purl.org/hercules/asio/core#requiredFor
asio:requiredFor rdf:type owl:ObjectProperty ;
                 rdfs:domain asio:AcademicPublication ;
                 rdfs:range asio:AcademicDegree ;
                 rdfs:label "required for"@en .


###  http://purl.org/hercules/asio/core#requires
asio:requires rdf:type owl:ObjectProperty ;
              rdfs:domain asio:AcademicDegree ,
                          asio:Role ;
              rdfs:range asio:AcademicPublication ,
                         asio:Role ;
              rdfs:comment "This property relates roles and their requisites, i.e. other roles."@en ;
              rdfs:label "requires"@en .


###  http://purl.org/hercules/asio/core#researchesIn
asio:researchesIn rdf:type owl:ObjectProperty ;
                  rdfs:domain foaf:Agent ;
                  rdfs:range modules:ScientificDomain ;
                  rdfs:label "researches in"@en .


###  http://purl.org/hercules/asio/core#reviews
asio:reviews rdf:type owl:ObjectProperty ;
             rdfs:domain foaf:Person ;
             rdfs:range asio:Publication ;
             rdfs:comment "a proerty to link a person to a publication as a reviewer."@en ;
             rdfs:label "reviews"@en .


###  http://purl.org/hercules/asio/core#specializes
asio:specializes rdf:type owl:ObjectProperty ;
                 rdfs:domain asio:Role ;
                 rdfs:range asio:Role ;
                 rdfs:comment "This property allows to express hierarchies of roles, through of role specialization: if role A specializes role B, and B specializes C, then role A specializes role C."@en ;
                 rdfs:label "specializes"@en .


###  http://purl.org/hercules/asio/core#spinsOffFrom
asio:spinsOffFrom rdf:type owl:ObjectProperty ;
                  rdfs:domain asio:Spinoff ;
                  rdfs:label "spins of from"@en .


###  http://purl.org/hercules/asio/core#student
asio:student rdf:type owl:ObjectProperty ;
             rdfs:domain foaf:Person ;
             rdfs:range asio:University ;
             rdfs:comment "A person studies or studied in a university."@en ;
             rdfs:label "student"@en .


###  http://purl.org/hercules/asio/core#taughtBy
asio:taughtBy rdf:type owl:ObjectProperty ;
              owl:inverseOf asio:teaches ;
              rdfs:label "taught by"@en .


###  http://purl.org/hercules/asio/core#teaches
asio:teaches rdf:type owl:ObjectProperty ;
             rdfs:domain foaf:Person ;
             rdfs:range asio:AcademicSubject ;
             rdfs:label "teaches"@en .


###  http://purl.org/hercules/asio/core#utilizes
asio:utilizes rdf:type owl:ObjectProperty ;
              rdfs:domain asio:ResearchContribution ;
              rdfs:range asio:ResearchMaterial ;
              rdfs:label "utilizes"@en .


###  http://purl.org/hercules/asio/core#yields
asio:yields rdf:type owl:ObjectProperty ;
            rdfs:domain asio:ResearchContribution ;
            rdfs:range asio:ResearchResult ;
            rdfs:label "yields"@en .


###  http://purl.org/wf4ever/ro#annotatesAggregatedResource
ro:annotatesAggregatedResource rdf:type owl:ObjectProperty ;
                               rdfs:label "annotates aggregated resource"@en .


###  http://www.w3.org/ns/prov#wasDerivedFrom
<http://www.w3.org/ns/prov#wasDerivedFrom> rdf:type owl:ObjectProperty ;
                                           rdfs:comment "A derivation is a transformation of an entity into another, an update of an entity resulting in a new one, or the construction of a new entity based on a pre-existing entity."@en ;
                                           rdfs:label "was derived from"@en .


###  http://xmlns.com/foaf/0.1/member
foaf:member rdf:type owl:ObjectProperty ;
            rdfs:domain foaf:Group ;
            rdfs:range foaf:Agent ;
            rdfs:label "member"@en .


###  https://www.w3.org/TR/prov-o/#wasDerivedFrom
<https://www.w3.org/TR/prov-o/#wasDerivedFrom> rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://purl.org/hercules/asio/core#awardedWith
asio:awardedWith rdf:type owl:DatatypeProperty ;
                 rdfs:domain asio:Publication ;
                 rdfs:range xsd:string ;
                 rdfs:label "awarded with"@en .


###  http://purl.org/hercules/asio/core#hasDOI
asio:hasDOI rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf asio:identifier ;
            rdfs:domain asio:Publication ;
            rdfs:range xsd:string ;
            rdfs:label "has DOI"@en .


###  http://purl.org/hercules/asio/core#hasORCID
asio:hasORCID rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf asio:identifier ;
              rdfs:domain foaf:Person ;
              rdfs:range xsd:string ;
              rdfs:label "has ORCID"@en .


###  http://purl.org/hercules/asio/core#hasResearcherID
asio:hasResearcherID rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf asio:identifier ;
                     rdfs:domain foaf:Person ;
                     rdfs:range xsd:string ;
                     rdfs:label "has Researcher ID"@en .


###  http://purl.org/hercules/asio/core#hasTRL
asio:hasTRL rdf:type owl:DatatypeProperty ;
            rdfs:domain asio:Prototype ;
            rdfs:range xsd:integer ;
            rdfs:comment "A prototype has a given technology readiness level"@en ;
            rdfs:label "has TRL"@en .


###  http://purl.org/hercules/asio/core#identifier
asio:identifier rdf:type owl:DatatypeProperty .


###  http://purl.org/hercules/asio/core#isActive
asio:isActive rdf:type owl:DatatypeProperty ;
              rdfs:domain asio:Profile ;
              rdfs:range xsd:boolean ;
              rdfs:comment """This property allows to specify which is the user profile that is active,
given a interaction between two applications."""@en ;
              rdfs:label "is active"@en .


###  http://purl.org/hercules/asio/core#isCompetitive
asio:isCompetitive rdf:type owl:DatatypeProperty ;
                   rdfs:domain foaf:Project ;
                   rdfs:range xsd:boolean ;
